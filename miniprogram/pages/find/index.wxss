.cavatar {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 20rpx;
  min-width: 150rpx;
}

.iconfont {
  font-size: 32rpx;
  margin-right: 10rpx;
}

.icon-male-01 {
  color: rgb(140, 158, 255);
}

.icon-nv {
  color: rgb(255, 77, 148)
}

.container {
  width: 100%;
  height: 300rpx;
  padding: 40rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}

.titleClass {
  font-size: 36rpx;
  font-weight: bold;
  line-height: 60rpx;
}

.gray-note {
  color: #aaa;
  margin-left: 20rpx;
  font-size: 20rpx;
}

.titleLabel {
  margin-left: 30rpx;
  margin-bottom: 20rpx;
}

.box {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-right: 30rpx;
}

.imgBox {
  position: relative;
}

.img {
  width: 160rpx;
  height: 160rpx;
  border-radius: 20rpx;
  margin-right: 10rpx;
}

.mask {
  position: absolute;
  top: 0;
  left: 0;
  width: 160rpx;
  height: 160rpx;
  border-radius: 20rpx;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 999;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 48rpx;
  color: #aaa;
}

.row {
  display: flex;
  flex-direction: row;
  align-items: flex-start;
}

.msg {
  display: flex;
  flex-direction: column;
}

.msg-label {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  margin-bottom: 20rpx;
}

.label-icon {
  color: rgb(41, 39, 77);
  margin-right: 10rpx;
}

.inner-label {
  line-height: 40rpx;
  margin-right: 20rpx;
  display: flex;
  align-items: flex-start;
}

.btn-icon {
  position: fixed;
  right: 50rpx;
  bottom: 400rpx;
  width: 100rpx;
  height: 100rpx;
  background-size: 100rpx 100rpx;
  background-repeat: no-repeat;
  background-image: url("data:image/svg+xml;base64,");
}

.close-icon{
  position: fixed;
  right: 50rpx;
  bottom: 280rpx;
  width: 100rpx;
  height: 100rpx;
  background-size: 100rpx 100rpx;
  background-repeat: no-repeat;
  background-image: url("data:image/svg+xml;base64,");
}